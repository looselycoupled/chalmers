<h1>Features#index</h1>
<p>Find me in app/views/features/index.html.erb</p>




<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Assets</th>
    </tr>
  </thead>
  <tbody>
    <% @features.each do |f| %>
    <tr>
      <td><%= link_to f.name, f %></td>
      <td><%= f.assignments.length %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New Feature", new_feature_path, class: 'btn btn-primary', role: 'btn' %>
